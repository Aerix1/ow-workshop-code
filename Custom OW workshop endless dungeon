		If(Event Player.randomRoll < 0.300);
			Create Dummy Bot(Hero Of(Event Player), Team 2, -1, Event Player, Vector(0, 0, 0));
			Set Status(Last Created Entity, Null, Phased Out, 1);
		End;
	}
