	event
	{
		Player Dealt Healing;
		Team 1;
		Zenyatta;
	}

	conditions
	{
		Event Ability == Button(Ability 1);
		Health Of Type(Healee, Shields) == 0;
	}

	actions
	{
		Add Health Pool To Player(Healee, Shields, Event Player.statHealing * 0.500, False, False);
		Wait(3, Ignore Condition);
	}
}

